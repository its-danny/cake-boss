{
   "name": "cake-boss",
   "version": "1.0.0",
   "description": "A discord bot that lets users reward each other cakes for being helpful, and redeem those cakes for prizes.",
   "scripts": {
      "dev": "NODE_ENV=development npx nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'npx ts-node' src/index.ts",
      "build": "NODE_ENV=production npx tsc",
      "start": "NODE_ENV=production node dist/index.js",
      "test": "NODE_ENV=test npx jest",
      "lint": "npx eslint --ext .ts ./",
      "pretty": "npx eslint --ext .ts ./ --fix",
      "typeorm": "npx ts-node ./node_modules/.bin/typeorm"
   },
   "author": "Danny Tatom",
   "license": "MIT",
   "private": true,
   "devDependencies": {
      "@types/cli-table": "^0.3.0",
      "@types/dotenv": "^6.1.1",
      "@types/faker": "^4.1.5",
      "@types/jest": "^24.0.17",
      "@types/koa": "^2.0.49",
      "@types/koa-router": "^7.0.42",
      "@types/koa__cors": "^2.2.3",
      "@types/node": "^12.7.1",
      "@types/node-schedule": "^1.2.3",
      "@typescript-eslint/eslint-plugin": "^1.13.0",
      "eslint": "^6.1.0",
      "eslint-config-airbnb-base": "^14.0.0",
      "eslint-config-airbnb-typescript": "^4.0.1",
      "eslint-config-prettier": "^6.0.0",
      "eslint-plugin-import": "^2.18.2",
      "eslint-plugin-jest": "^22.15.0",
      "eslint-plugin-prettier": "^3.1.0",
      "faker": "^4.1.0",
      "jest": "^24.8.0",
      "nodemon": "^1.19.1",
      "prettier": "^1.18.2",
      "ts-jest": "^24.0.2",
      "ts-node": "8.3.0",
      "typescript": "3.5.3"
   },
   "dependencies": {
      "@koa/cors": "^3.0.0",
      "@sentry/node": "^5.6.1",
      "cli-table": "^0.3.1",
      "discord.js": "^11.5.1",
      "dotenv": "^8.0.0",
      "koa": "^2.7.0",
      "koa-router": "^7.4.0",
      "koa-socket": "^4.4.0",
      "moment": "^2.24.0",
      "node-schedule": "^1.3.2",
      "pg": "^7.12.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.18",
      "yargs": "^14.0.0"
   }
}
