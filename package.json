{
  "name": "cake-boss",
  "version": "1.0.0",
  "description": "A Discord bot that lets users reward each other cakes for being helpful, and redeem those cakes for prizes.",
  "author": "Danny Tatom",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development yarn run nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'yarn run ts-node' src/index.ts",
    "build": "NODE_ENV=production yarn run tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "NODE_ENV=test yarn run jest",
    "lint": "yarn run eslint --ext .ts ./",
    "pretty": "yarn run eslint --ext .ts ./ --fix && yarn run pretty-quick --staged",
    "typeorm": "yarn run ts-node ./node_modules/.bin/typeorm",
    "commit": "npx git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pretty"
    }
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^24.0.22",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^6.1.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sqlite3": "^4.1.0",
    "ts-jest": "^24.1.0"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@sentry/node": "^5.8.0",
    "@types/cli-table": "^0.3.0",
    "@types/faker": "^4.1.7",
    "@types/fs-extra": "^8.0.1",
    "@types/json2csv": "^4.5.0",
    "@types/koa": "^2.0.52",
    "@types/koa-router": "^7.0.42",
    "@types/koa__cors": "^2.2.3",
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@types/node-schedule": "^1.2.4",
    "@types/underscore.string": "^0.0.38",
    "axios": "^0.19.0",
    "cli-table": "^0.3.1",
    "csvtojson": "^2.0.10",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "json2csv": "^4.5.4",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.5.0",
    "typeorm": "^0.2.20",
    "typescript": "^3.7.2",
    "underscore.string": "^3.3.5",
    "yargs": "^14.2.0"
  }
}
