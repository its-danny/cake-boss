{
  "name": "cake-boss",
  "version": "0.0.0",
  "description": "A Discord bot that lets users reward each other cakes for being helpful, and redeem those cakes for prizes.",
  "author": "Danny Tatom",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node' src/index.ts",
    "build": "NODE_ENV=production tsc -p tsconfig-build.json",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext .ts src/ test/",
    "fix": "eslint --ext .ts src/ test/ --fix && prettier --write '**/*.ts' src/ test/",
    "typeorm": "ts-node ./node_modules/.bin/typeorm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn fix && yarn test",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/faker": "^4.1.12",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.10",
    "@types/json2csv": "^5.0.1",
    "@types/koa": "^2.11.4",
    "@types/koa-router": "^7.4.1",
    "@types/koa__cors": "^3.0.1",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.6.0",
    "@types/node-schedule": "^1.3.0",
    "@types/underscore.string": "^0.0.38",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "commitizen": "^4.1.5",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sqlite3": "^5.0.0",
    "ts-jest": "^26.2.0"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@sentry/node": "^5.21.1",
    "axios": "^0.20.0",
    "cli-table": "^0.3.1",
    "csvtojson": "^2.0.10",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "json2csv": "^5.0.1",
    "koa": "^2.13.0",
    "koa-router": "^9.4.0",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "node-schedule": "^1.3.2",
    "pg": "^8.3.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.7",
    "underscore.string": "^3.3.5",
    "yargs": "^15.4.1"
  }
}
