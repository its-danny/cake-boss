{
  "name": "cake-boss",
  "version": "0.0.0",
  "description": "A Discord bot that lets users reward each other cakes for being helpful, and redeem those cakes for prizes.",
  "author": "Danny Tatom",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node' src/index.ts",
    "build": "NODE_ENV=production tsc -p tsconfig-build.json",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext .ts src/ test/",
    "fix": "eslint --ext .ts src/ test/ --fix && prettier --write '**/*.ts' src/ test/",
    "typeorm": "ts-node ./node_modules/.bin/typeorm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn fix && yarn test",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/faker": "^5.1.7",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/json2csv": "^5.0.1",
    "@types/koa": "^2.13.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa__cors": "^3.0.2",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/node-schedule": "^1.3.1",
    "@types/underscore.string": "^0.0.38",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "^5.4.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sqlite3": "^5.0.2",
    "ts-jest": "^26.5.4"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@sentry/node": "^6.2.0",
    "axios": "^0.21.1",
    "cli-table": "^0.3.5",
    "csvtojson": "^2.0.10",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.1.0",
    "json2csv": "^5.0.6",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "node-schedule": "^2.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.31",
    "typescript": "^4.1.5",
    "underscore.string": "^3.3.5",
    "yargs": "^16.2.0"
  }
}
